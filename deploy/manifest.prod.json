{
  "$schema": "https://developer.microsoft.com/json-schemas/teams/v1.19/MicrosoftTeams.schema.json",
  "manifestVersion": "1.19",
  "version": "1.0.3",
  "id": "224ba22b-cdfb-4fca-9e3b-f5b3051c73b3",
  "developer": {
    "name": "Valantic CEC Deutschland GmbH",
    "websiteUrl": "https://www.valantic.com/de/cx/",
    "privacyUrl": "https://www.valantic.com/de/datenschutzerklaerung",
    "termsOfUseUrl": "https://www.valantic.com/de/impressum-und-rechtshinweise"
  },
  "icons": {
    "outline": "outline.png",
    "color": "color.jpg"
  },
  "name": {
    "short": "Workoflow Bot",
    "full": "Efficiency is yours."
  },
  "description": {
    "short": "Efficiency is yours.",
    "full": "Company automations made easy and accessible. Identify, automate, publish, repeat. (powered by n8n: https://workflows.vcec.cloud/)."
  },
  "accentColor": "#ff4b4b",
  "bots": [
    {
      "botId": "ec40bbb0-909e-4b46-908f-ce9b37735e5f",
