        "personal",
        "groupChat",
        "team"
      ],
      "isNotificationOnly": false,
      "supportsCalling": false,
      "supportsVideo": false,
      "supportsFiles": true,
      "commandLists": [
        {
          "scopes": [
            "personal",
            "groupChat",
            "team"
          ],
          "commands": [
            {
              "title": "Which tools you have connected?",
              "description": "Tell me about the internal tools you have access for."
            },
            {
              "title": "What are missing in Ticket XY-9?",
              "description": "Use the Bot to ask ticket related questions."
            }
          ]
        }
      ]
    }
  ],
  "permissions": [
    "identity",
    "messageTeamMembers"
  ],
  "authorization": {
    "permissions": {
      "resourceSpecific": [
        {
          "name": "ChatMessageReadReceipt.Read.Chat",
          "type": "Application"
        }
      ]
    }
  },
  "validDomains": [
    "*.ngrok-free.app"
